{
    "name": "uncss",
    "displayName": "UnCSS",
    "description": "HIghlights and removes unused CSS rules",
    "version": "0.0.1",
    "publisher": "Safiyya",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.highlight",
                "title": "UnCSS : highlight"
            }
        ],
        "keybindings": {
            "command": "extension.highlight",
            "key": "ctrl+shift+R",
            "mac": "shift+cmd+R"
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "purify-css": "^1.2.5"
    }
}
